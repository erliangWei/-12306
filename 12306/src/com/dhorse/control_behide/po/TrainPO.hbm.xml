<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.dhorse.control_behide.po">
	<!-- name表示类名  table表示表名-->
	<class name="TrainPO" table="TRAIN">
		<!-- 车次代码 -->
		<id name="trainId" column="TRAINID">
			 <generator  class="increment"/> 
		</id>
		<!-- 始发站 -->
		<property name="trainCode" column="TRAINCODE"  />
		<property name="statrStation" column="STARTSTATION"  />
		<!-- 终点站 -->
		<property name="endStation" column="ENDSTATION"  />
		<!-- 始发时间-->
		<property name="startTime" column="STARTTIME"  />
		<!-- 终点时间 -->
		<property name="endTime" column="ENDTIME"  />
		<!-- 历时 -->
		<property name="totalTime" column="TOTALTIME"  />
		<!-- 里程 -->
		<property name="totalDistance" column="TOTALDISTANCE"  />
		<!-- 无座票数量 -->
		<property name="standTicketNum" column="STANDTICKET"  />
		<!-- 车体分类 -->
		<property name="trainBody" column="TRAINBODY"  />
		<!-- 列车类型 -->
		<property name="trainType" column="TRAINTYPE"  />
		
		</class>
		</hibernate-mapping>